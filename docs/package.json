{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "description": "Documentation for @nrg-ui/core addon",
  "repository": {
    "directory": "docs",
    "type": "git",
    "url": "https://github.com/knoxville-utilities-board/nrg-ui.git"
  },
  "license": "MIT",
  "author": {
    "name": "Knoxville Utilities Board",
    "url": "https://www.kub.org"
  },
  "imports": {
    "#app/*": "./app/*"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefix-colors=auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefix-colors=auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:intl": "ember-intl-lint",
    "lint:intl:fix": "ember-intl-lint --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "start": "vite",
    "test": "vite build --mode development && ember test --path dist"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.6",
    "@babel/runtime": "^7.28.6",
    "@ember-intl/lint": "^0.4.1",
    "@ember-intl/vite": "^0.4.0",
    "@ember/app-tsconfig": "^2.0.0",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/compat": "^4.1.11",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.3.0",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@embroider/macros": "^1.19.6",
    "@embroider/router": "^3.0.6",
    "@embroider/vite": "^1.5.0",
    "@floating-ui/dom": "^1.7.4",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.0.9",
    "@glint/template": "^1.7.3",
    "@nrg-ui/code-snippets": "workspace:*",
    "@nrg-ui/core": "workspace:*",
    "@nrg-ui/css": "workspace:*",
    "@nrg-ui/showcase": "workspace:*",
    "@nrg-ui/standards": "workspace:*",
    "@nrg-ui/version": "workspace:*",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/qunit": "^2.19.13",
    "babel-plugin-ember-template-compilation": "^4.0.0",
    "concurrently": "^9.2.1",
    "dayjs": "^1.11.19",
    "decorator-transforms": "^2.3.1",
    "ember-cli": "~6.8.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-deprecation-workflow": "^4.0.0",
    "ember-concurrency": "^5.1.0",
    "ember-css-transitions": "^4.5.0",
    "ember-intl": "^8.0.3",
    "ember-keyboard": "^9.0.4",
    "ember-lifeline": "^7.0.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.8.0",
    "ember-template-lint": "^7.9.3",
    "ember-try": "^4.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-decorator-position": "^6.0.0",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-qunit": "^8.2.5",
    "ismobilejs": "^1.1.1",
    "prettier": "^3.8.1",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.5.0",
    "stylelint": "^17.0.0",
    "stylelint-config-standard": "^40.0.0",
    "testem": "^3.16.0",
    "tracked-built-ins": "^4.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  }
}
