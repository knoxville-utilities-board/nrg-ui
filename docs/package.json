    "@glint/ember-tsc": "^1.0.7",
    "@glint/template": "^1.7.2",
    "@nrg-ui/core": "workspace:*",
    "@nrg-ui/css": "workspace:*",
    "@nrg-ui/showcase": "workspace:*",
    "@nrg-ui/standards": "workspace:*",
    "@nrg-ui/version": "workspace:*",
    "@tsconfig/ember": "^3.0.12",
    "@types/qunit": "^2.19.13",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.1",
