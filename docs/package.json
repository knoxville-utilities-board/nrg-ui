    "qunit-dom": "^3.4.0",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
