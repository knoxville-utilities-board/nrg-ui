<div class="container p-3">
  <div class="index-nav-list list-group">
    <span class="list-group-item active" aria-current="true">Navigation</span>
    {{#each this.routes as |route|}}
      <LinkTo @route={{route}} class="list-group-item list-group-item-action">
        {{route}}
      </LinkTo>
    {{/each}}
  </div>
</div>