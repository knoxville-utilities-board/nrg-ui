{
  "name": "@nrg-ui/core",
  "version": "0.56.0",
  "description": "Design system for Ember.js built on top of Bootstrap",
  "keywords": [
    "bootstrap",
    "design",
    "design-system",
    "ember",
    "ember-addon"
  ],
  "repository": {
    "directory": "packages/ember-core",
    "type": "git",
    "url": "https://github.com/knoxville-utilities-board/nrg-ui.git"
  },
  "license": "MIT",
  "author": {
    "name": "Knoxville Utilities Board",
    "url": "https://www.kub.org"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./test-support": {
      "types": "./declarations/test-support/index.d.ts",
      "default": "./dist/test-support/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.cjs",
    "./components": {
      "types": "./declarations/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./components/form": {
      "types": "./declarations/components/form/index.d.ts",
      "default": "./dist/components/form/index.js"
    },
    "./helpers": {
      "types": "./declarations/helpers/index.d.ts",
      "default": "./dist/helpers/index.js"
    },
    "./modifiers": {
      "types": "./declarations/modifiers/index.d.ts",
      "default": "./dist/modifiers/index.js"
    },
    "./utils": {
      "types": "./declarations/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./validation": {
      "types": "./declarations/validation/index.d.ts",
      "default": "./dist/validation/index.js"
    },
    "./template-registry": {
      "types": "./declarations/template-registry.d.ts"
    },
    "./service-registry": {
      "types": "./declarations/service-registry.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  },
  "files": [
    "addon-main.cjs",
    "blueprints",
    "declarations",
    "dist",
    "translations"
  ],
  "scripts": {
    "build": "concurrently 'pnpm:build:*'",
    "build:icons": "node lib/build-icons.mjs",
    "build:js": "rollup --config",
    "build:types": "concurrently 'pnpm:build:types:*' --names 'types:'",
    "build:types:glint": "ember-tsc --declaration",
    "format": "prettier . --cache --write",
    "generate": "pnpm exec gember",
    "lint": "concurrently 'pnpm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefix-colors=auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:intl": "ember-intl-lint",
    "lint:intl:fix": "ember-intl-lint --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "prepack": "concurrently 'pnpm:build:*'",
    "start": "concurrently 'pnpm:start:*'",
    "start:js": "rollup --config --watch --no-watch.clearScreen",
    "start:types": "ember-tsc --declaration --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in docs'"
  },
  "dependencies": {
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/addon-shim": "^1.10.2",
    "@embroider/macros": "^1.19.6",
    "@floating-ui/dom": "^1.7.4",
    "chalk": "^4.1.2",
    "decorator-transforms": "^2.3.1",
    "ember-css-transitions": "^4.5.0",
    "ember-intl": "^8.0.3",
    "ember-keyboard": "^9.0.4",
    "ember-lifeline": "^7.0.0",
    "ember-modifier": "^4.2.2",
    "ismobilejs": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-typescript": "^7.28.6",
    "@babel/runtime": "7.28.6",
    "@bertdeblock/gember": "^1.6.0",
    "@ember-intl/lint": "^0.4.1",
    "@embroider/addon-dev": "^8.2.0",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.0.9",
    "@glint/template": "^1.7.3",
    "@nrg-ui/css": "workspace:^",
    "@nrg-ui/standards": "workspace:^",
    "@rollup/plugin-babel": "^6.1.0",
    "@tsconfig/ember": "^3.0.12",
    "babel-plugin-ember-template-compilation": "^4.0.0",
    "concurrently": "^9.2.1",
    "dayjs": "^1.11.19",
    "ember-cli": "~5.12.0",
    "ember-concurrency": "^5.1.0",
    "ember-template-lint": "^7.9.3",
    "ember-truth-helpers": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-decorator-position": "^6.0.0",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.2",
    "execa": "^9.6.1",
    "prettier": "^3.8.1",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "rollup": "^4.56.0",
    "rollup-plugin-copy": "^3.5.0",
    "tracked-built-ins": "^4.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "peerDependencies": {
    "@glimmer/component": "^2.0.0",
    "@nrg-ui/css": "workspace:^0",
    "dayjs": "^1.11.19",
    "ember-concurrency": "^5.1.0",
    "ember-css-transitions": "^4.4.0",
    "ember-intl": "^7.0.4 || ^8.0.1",
    "ember-source": "^5.12.0 || ^6.0.0",
    "ember-truth-helpers": "^5.0.0",
    "tracked-built-ins": "^4.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
